# QuickBooks OAuth Configuration (Static Settings Only)
QB_BASE_URL=https://sandbox-quickbooks.api.intuit.com
QB_CLIENT_ID=your_client_id_here
QB_CLIENT_SECRET=your_client_secret_here
QB_REDIRECT_URI=http://localhost:8000/callback
# Note: QB_COMPANY_ID will be stored in tokens.json after OAuth flow
# Note: Access and refresh tokens are now stored in data/tokens.json

# Application Settings
APP_NAME=quickexpense
APP_VERSION=0.1.0
DEBUG=false
LOG_LEVEL=INFO

# API Settings
API_PREFIX=/api/v1
CORS_ORIGINS=["http://localhost:3000"]

# Gemini AI Settings
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_TIMEOUT=30
